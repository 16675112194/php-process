{
  "name": "wanglelecc/php-process",
  "description": "Multi process manager for PHP",
  "type": "project",
  "homepage": "https://php-process.56br.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "wanglelecc",
      "email": "wanglelecc@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-pcntl": "*",
    "ext-posix": "*"
  },
  "autoload": {
    "classmap": [

    ],
    "psr-4": {
      "App\\": "./app",
      "Wanglelecc\\": "./src"
    },
    "files" : [
      "./app/helpers.php"
    ]
  },
  "scripts": {
    "post-root-project-cmd": [
      "cp .env.example .env"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}